{
	"name": "client",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "node ./server/src/index.js",
		"start-sudo": "PORT=80 node ./server/src/index.js",
		"test": "jest",
		"dev": "export NODE_ENV=development && webpack --mode=development --progress && npm run server",
		"build": "export NODE_ENV=production && webpack --mode=production --progress",
		"server": "nodemon --watch ./server/src --inspect=0.0.0.0:3001 ./server/src/index.js --exec 'npm run inspect'",
		"inspect": "kill-port --port 3001 && node --inspect=0.0.0.0:3001 server/src/index.js"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"nodemon": "1.19.1",
		"webpack": "4.39.1",
		"webpack-cli": "3.3.6",
		"webpack-dev-middleware": "3.7.2",
		"webpack-dev-server": "3.8.0",
		"webpack-hot-middleware": "2.25.0",
		"webpack-merge": "4.2.2",
		"html-webpack-plugin": "3.2.0",
		"optimize-css-assets-webpack-plugin": "5.0.3",
		"html-loader": "0.5.5",
		"eslint": "6.5.1",
		"kill-port": "1.6.0",
		"file-loader": "4.2.0",
		"style-loader": "1.0.0",
		"css-loader": "3.2.0",
		"sass-loader": "8.0.0",
		"node-sass": "4.13.0",
		"mini-css-extract-plugin": "0.8.0"
	},
	"dependencies": {
		"axios": "0.19.0",
		"express": "4.17.1"
	}
